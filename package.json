{
  "name": "joist",
  "version": "0.0.3",
  "description": "A GraphQL boilerplate using graphql-modules and express",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed-data": "knex seed:run --specific testData.js",
