    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "faker": "^4.1.0",
    "graphql": "^14.5.8",
    "graphql-import-node": "0.0.4",
    "graphql-input-string": "0.0.16",
    "graphql-tag": "^2.10.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.4.0",
    "lodash": "^4.17.15",
    "mysql": "^2.17.1",
